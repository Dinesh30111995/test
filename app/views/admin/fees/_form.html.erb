    <!--</div>-->
  <!--</div>-->

   <div class="form-group row">
    <label for="example-text-input" class="col-md-2 col-form-label"><%= t("admin.fees.form.per_mile") %></label>
    
    <div class="col-md-10">
      <%= text_field_tag 'fee[per_mile]', @fee.per_mile, class: 'form-control input-custom', placeholder: t('admin.fees.form.per_mile'), required: true %>
    </div>
  </div>

  


  <div class="form-group row">
    <label for="example-number-input" class="col-md-2 col-form-label"></label>
