  <h4 class="card-title"><%= title %></h4>
  <div class="form-group row">
    <label for="example-text-input" class="col-md-2 col-form-label"><%= t("admin.fees.form.fee_type") %></label>
    <div class="col-md-10">
    <%= select_tag "fee[fee_type]", options_for_select(Fee.fee_types.keys.collect{|x| [x]},@fee.fee_type), {prompt: "--Select--",class: 'form-control input-custom', id: 'fee_type', required: true} %>
    </div>
  </div>

