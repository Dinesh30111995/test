<div class="asset-card">
  <%= render 'feed/asset_pro_details', collection:collection %>
  <div class="promote-feed">
    <a href="<%= collection_path(collection.address)%>" target="_blank">
      <div class="assets_section audio-format">
        <%= audio_tag url_for(collection.attachment), size: "550x400", controls: true %>
      </div>
    </a>
  </div>
  <%= render 'feed/asset_detail', collection:collection %>
  <%= render 'feed/asset_footer', collection:collection %>
</div>